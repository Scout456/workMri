			});
			$(elem).each(function () {

				if (window.location.href.includes("#")) {
					const [videoID, videoIDVal] = window.location.href.split("#")[1].split("-");
					const videoCarouselID = $(this).attr('id');
			
					if (videoID === videoCarouselID) {
